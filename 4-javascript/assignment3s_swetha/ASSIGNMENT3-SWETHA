             CELCIUS TO FARENHEIT CONVERSION        

<!DOCTYPE html>
<html>
<head>
	<title>Conversion</title>
</head>
<body>
	<h1>celcius to farnheit</h1>
	<script type="text/javascript">
		function Conversion(celcius){
			a=(celcius*9/5)+32;
			console.log('celcius to farnheit value is'+a+"f");
		}
		function Conversion(farnheit){
			a=(farnheit-32)*5/9;
			console.log('farenheit to celcius value is'+a+"c");
		}
		a=Conversion(78);
		a=Conversion(54);
	</script>

</body>
</html>
