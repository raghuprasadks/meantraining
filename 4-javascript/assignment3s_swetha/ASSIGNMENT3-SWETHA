                         EB BILL FOR A HOUSE

<!DOCTYPE html>
<html>
<head>
	<title>EB BILL</title>
</head>
<body>
	<h1>EB BILL FOR A HOUSE</h1>
	<script type="text/javascript">
		function Ebbill(unit){
			if(unit<=100){
				result=unit*2;
				console.log('EB bill is'+result);
			}
			else if(unit>100&&unit<=200){
				result=unit*3;
				console.log('EB bill is'+result);
			}

			else if(unit>200){
				result=unit*4;
				console.log('EB bill is'+result);

			}
			else{
				console.log('EB bill not available')
			}

			}
			res=Ebbill(600);
		</script>

		

</body>
</html>
